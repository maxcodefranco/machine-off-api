  "info": {
    "title": "Machine.off API",
    "description": "API for event invitation and RSVP management",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "AdminCreateEventInputDto": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "location": {
            "type": "string"
          },
          "startDate": {
            "type": "string"
          },
          "endDate": {
            "type": "string"
          },
          "startTime": {
            "type": "string"
          },
          "endTime": {
            "type": "string"
          },
          "shiftOptions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": {
                  "type": "string"
                },
                "label": {
                  "type": "string"
                },
                "startTime": {
                  "type": "string"
                },
                "endTime": {
                  "type": "string"
                }
              },
              "required": [
                "key",
                "label",
                "startTime",
                "endTime"
              ]
            }
          },
          "orientacoes": {
            "type": "string"
          },
          "deadline": {
            "type": "string"
          },
          "pricingType": {
            "type": "string",
            "enum": [
              "fixed",
              "split"
            ]
          },
          "pricingValue": {
            "type": "number"
          },
          "links": {
            "type": "object"
          }
        },
        "required": [
          "slug",
          "name",
          "location",
          "startDate",
          "startTime",
          "endTime",
          "orientacoes"
        ]
